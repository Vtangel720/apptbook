<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.LoginController">
   <children>
      <TextField fx:id="userNameFld" alignment="CENTER" layoutX="178.0" layoutY="82.0" />
      <TextField fx:id="passwordFld" alignment="CENTER" layoutX="178.0" layoutY="132.0" />
      <Button fx:id="loginBtn" layoutX="126.0" layoutY="200.0" mnemonicParsing="false" onAction="#handleLogin" text="Login" />
      <Button fx:id="resetBtn" layoutX="214.0" layoutY="200.0" mnemonicParsing="false" onAction="#handleReset" text="Reset" />
      <Button fx:id="exitBtn" layoutX="284.0" layoutY="259.0" mnemonicParsing="false" onAction="#handleExit" prefHeight="27.0" prefWidth="110.0" text="Exit" />
      <Text fx:id="loginTxt" layoutY="38.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Login" textAlignment="CENTER" wrappingWidth="400.0" />
      <Text fx:id="usernameTxt" layoutX="11.0" layoutY="100.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username:" textAlignment="RIGHT" wrappingWidth="138.0" />
      <Text fx:id="passwordTxt" layoutX="11.0" layoutY="150.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password:" textAlignment="RIGHT" wrappingWidth="138.0" />
   </children>
</AnchorPane>
